<h1>Edit Your Profile</h1>

<%= form_for @user do |f|%>
    <%= f.label :username %>
    <%= f.text_field :username %> <br>

    <%= f.label :first_name%>
    <%= f.text_field :first_name %> <br>

    <%= f.label :last_name%>
    <%= f.text_field :last_name %> <br>

    <%= f.label :img_url%>
    <%= f.text_field :img_url %> <br>

    <%=f.submit "Update"%>

<%end%>